{
	"name": "@flint.fyi/core",
	"version": "0.16.0",
	"description": "[Experimental] Core building blocks for Flint.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JoshuaKGoldberg/flint.git",
		"directory": "packages/core"
	},
	"license": "MIT",
	"author": {
		"name": "JoshuaKGoldberg",
		"email": "npm@joshuakgoldberg.com"
	},
	"sideEffects": [
		"./lib/ts-patch/install-patch.js",
		"./lib/ts-patch/install-patch-hooks.js",
		"./lib/ts-patch/proxy-program.ts"
	],
	"type": "module",
	"exports": {
		".": {
			"@flint/source": "./src/index.ts",
			"default": "./lib/index.js"
		},
		"./package.json": "./package.json",
		"./ts-patch/install-patch": {
			"@flint/source": "./src/ts-patch/install-patch.ts",
			"default": "./lib/ts-patch/install-patch.js"
		},
		"./ts-patch/install-patch-hooks": {
			"@flint/source": "./src/ts-patch/install-patch-hooks.ts",
			"default": "./lib/ts-patch/install-patch-hooks.js"
		},
		"./ts-patch/proxy-program": {
			"@flint/source": "./src/ts-patch/proxy-program.ts",
			"default": "./lib/ts-patch/proxy-program.js"
		}
	},
	"main": "./lib/index.js",
	"module": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/"
	],
	"dependencies": {
		"@flint.fyi/utils": "workspace:",
		"cached-factory": "^0.1.0",
		"debug-for-file": "^0.2.0",
		"omit-empty": "^1.0.0",
		"prettier": "3.7.4",
		"typescript": "^5.9.3 || ^6.0.0",
		"zod": "^4.2.1"
	},
	"devDependencies": {
		"@types/omit-empty": "1.0.4",
		"vitest": "4.0.15"
	},
	"engines": {
		"node": ">=24.0.0"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": "./lib/index.js",
			"./ts-patch/install-patch": "./lib/ts-patch/install-patch.js",
			"./ts-patch/install-patch-hooks": "./lib/ts-patch/install-patch-hooks.js",
			"./ts-patch/proxy-program": "./lib/ts-patch/proxy-program.js",
			"./package.json": "./package.json"
		},
		"provenance": true
	}
}
