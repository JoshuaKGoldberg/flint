{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Run Current File Tests",
			"program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
			"args": ["${fileBasenameNoExtension}"],
			"sourceMaps": true,
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		}
	]
}
