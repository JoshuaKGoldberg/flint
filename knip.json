{
	"$schema": "https://unpkg.com/knip@5.61.2/schema.json",
	"ignore": ["flint.config.*", "packages/fixtures/*"],
	"ignoreDependencies": ["flint", "^@flint\\.fyi\\/.*"],
	"ignoreExportsUsedInFile": { "interface": true, "type": true },
	"workspaces": {
		"packages/core": {
			"entry": [
				"src/ts-patch/install-patch.ts",
				"src/ts-patch/install-patch-hooks.ts"
			]
		},
		"packages/comparisons": {
			"entry": ["src/sort-data.ts"]
		},
		"packages/site": {
			"entry": ["src/components/*.astro"],
			"ignoreDependencies": ["sharp"]
		}
	}
}
